<div class="separator">
    <span id="toggle_services_list" style="cursor: pointer;">Services &nbsp; &#9660;</span>
</div>
<div id="services_list">
    {% for service in specter.service_manager.services %}
        {% if service.active and service.has_blueprint %}
        <a href="{{ url_for(service.id +'_endpoint.index') }}" class="item service">
            <img src="{{ url_for(service.id +'_endpoint' + '.static', filename=service.logo) }}" height="30px">
        </a>
        <br>
        {% endif %}
    {% endfor %}
</div>
