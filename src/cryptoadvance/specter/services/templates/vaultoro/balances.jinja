{% extends "vaultoro/components/vaultoro_tab.jinja" %}
{% set tab = 'balances' %}
{% block content %}

<vaultoro-balances></vaultoro-balances>

<h1 class="padded">History</h1>
<div class="card" style="width: 610px; padding-top: 25px;">
    <div class="historyDiv">
        <table>
            <tr>
                <th>createdAt</th>
                <th>matchType</th>
                <th>pair</th>
                <th>price</th>
                <th>quantity</th>
                <th>type</th>
            </tr>
            {% for item in history["data"] %}
            <tr>
                <th>{{ item.createdAt }}</th>
                <th>{{ item.matchType }}</th>
                <th>{{ item.pair }}</th>
                <th>{{ item.price }}</th>
                <th>{{ item.quantity }}</th>
                <th>{{ item.type }}</th>
            </tr>
            {% endfor %}
        </table>
    </div>
</div>

<hr>
<h4> Debug</h4>
<h5>Balances</h5>
<p >
{{balances}}
</p>

<h5>History</h5>
{{history}}


{% endblock %}
{% block scripts %}
{% endblock %}